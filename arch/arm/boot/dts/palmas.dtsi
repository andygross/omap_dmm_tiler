				ti,warm_sleep = <0>;
				ti,roof_floor = <0>;
				ti,mode_sleep = <0>;
				ti,warm_reset = <0>;
				ti,tstep = <0>;
				ti,vsel = <0>;
			};

			smps8_reg: smps8 {
				regulator-name = "smps8";
				regulator-min-microvolt = < 600000>;
				regulator-max-microvolt = <1310000>;
				regulator-always-on;
				regulator-boot-on;
				ti,warm_sleep = <0>;
				ti,roof_floor = <0>;
