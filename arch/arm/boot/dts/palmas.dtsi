				regulator-max-microvolt = <2100000>;
				regulator-always-on;
				regulator-boot-on;
				ti,warm_sleep = <0>;
				ti,roof_floor = <0>;
				ti,mode_sleep = <0>;
				ti,warm_reset = <0>;
				ti,tstep = <0>;
				ti,vsel = <0x80>;
			};

			smps10_reg: smps10 {
				regulator-name = "smps10";
				regulator-min-microvolt = <5000000>;
