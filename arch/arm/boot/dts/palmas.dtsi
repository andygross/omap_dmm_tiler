				regulator-name = "ldo6";
				regulator-min-microvolt = <1500000>;
				regulator-max-microvolt = <1500000>;
				regulator-always-on;
				regulator-boot-on;
				ti,warm_sleep = <0>;
				ti,roof_floor = <0>;
